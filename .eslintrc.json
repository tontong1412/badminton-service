{
  "env": {
      "jest": true
  },
  "extends": "airbnb-base",
  "plugins": [
      "import",
      "jest"
  ],
  "rules": {
      "semi": [
          "error",
          "never"
      ],
      "no-underscore-dangle":"off",
      "camelcase":"off",
      "no-param-reassign":"off",
      "import/first":"off"
  }
}